        <Label Content="ImageSeq" HorizontalAlignment="Left" Height="29" Margin="10,165,0,0" VerticalAlignment="Top" Width="69"/>
        <TextBox x:Name="txtIQ" HorizontalAlignment="Left" Height="20" Margin="10,199,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="80"/>
        <Button x:Name="btnIQ" Content="Find" HorizontalAlignment="Left" Height="20" Margin="98,199,0,0" VerticalAlignment="Top" Width="40" Click="BtnVS_Click"/>
        <Button Content="Save" HorizontalAlignment="Left" Height="23" Margin="154,256,0,0" VerticalAlignment="Top" Width="48" Click="Button_Click"/>

    </Grid>
</Window>
