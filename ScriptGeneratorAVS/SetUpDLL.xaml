        mc:Ignorable="d"
        Title="SetupDLL" Height="320" Width="220" Closing="Window_Closing">
    <Grid>
        <Label Content="VsFilterMod" HorizontalAlignment="Left" Height="29" Margin="10,25,0,0" VerticalAlignment="Top" Width="80"/>
        <TextBox x:Name="txtVS" HorizontalAlignment="Left" Height="20" Margin="10,59,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="80"/>
        <Button x:Name="btnVS" Content="Find" HorizontalAlignment="Left" Height="20" Margin="98,59,0,0" VerticalAlignment="Top" Width="40" Click="BtnVS_Click"/>
        <Label Content="ffms2" HorizontalAlignment="Left" Height="29" Margin="10,90,0,0" VerticalAlignment="Top" Width="69"/>
